{
    "globalTeardown": "<rootDir>/jest.teardown.js",
    "collectCoverage": true,
    "coverageReporters": [
        "json-summary",
        "text"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 80
        }
    },
    "testPathIgnorePatterns": ["/node_modules/", "/dist/"],
    "coveragePathIgnorePatterns": [
        "config.js", "/dist/", "/node_modules/", "src/logger.js"
    ]
}