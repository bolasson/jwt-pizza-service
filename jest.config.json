{
    "globalTeardown": "<rootDir>/jest.teardown.js",
    "collectCoverage": true,
    "coverageReporters": [
        "json-summary",
        "text"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 80
        }
    },
    "coveragePathIgnorePatterns": [
        "config.js"
    ]
}